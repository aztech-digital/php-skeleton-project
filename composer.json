{
  "require": {
    "symfony/console": "~2",
    "composer/composer": "dev-master",
    "aztech/php-utils": "dev-master"
  },

  "require-dev": {
    "phpunit/phpunit": "~4",
    "squizlabs/php_codesniffer": "~1"
  },

  "scripts": {
    "post-create-project-cmd": ["Aztech\\Skeleton\\Commands\\InstallCommand::postCreateProjectInstall"]
  },

  "autoload": {
    "psr-4": {
      "Aztech\\Skeleton\\": "src/"
    }
  },

  "name": "aztech/php-skeleton-project",
  "description": "Skeleton for PHP projects",
  "type": "project",
  "homepage": "https://github.com/aztech-digital/php-skeleton-project"
}
